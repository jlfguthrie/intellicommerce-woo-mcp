    "@eslint/js": "^9.28.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.10",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.1.0",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.10",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.3.4",
    "tsx": "^4.7.3",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
