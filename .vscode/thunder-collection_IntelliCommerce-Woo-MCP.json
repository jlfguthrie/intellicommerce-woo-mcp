  "requests": [
    {
      "id": "woo-test-connection",
      "name": "Test WooCommerce Connection",
      "url": "{{woo_url}}/wp-json/wc/v3/system_status",
      "method": "GET",
      "sortNum": 10000,
      "containerId": "woo-auth",
      "headers": [
        {
          "name": "Authorization",
          "value": "Basic {{woo_auth_token}}"
        }
      ],
      "tests": [
        {
          "type": "res-code",
          "custom": "",
          "action": "equal",
          "value": "200"
        }
      ]
    }
  ],
  "environments": [
    {
      "id": "woo-dev",
      "name": "üõ†Ô∏è WooCommerce Development",
