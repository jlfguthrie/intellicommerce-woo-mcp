{
  "name": "intellicommerce-woo-mcp",
  "version": "1.1.13",
  "description": "âœ¨IntelliCommerceâœ¨ Woo MCP - A commercial-grade Model Context Protocol server for intelligent WooCommerce interactions. Made with ðŸ§¡ in Cape Town ðŸ‡¿ðŸ‡¦",
  "type": "module",
  "main": "./build/server.js",
  "exports": "./build/server.js",
  "bin": {
    "intellicommerce-woo-mcp": "bin/intellicommerce-woo-mcp"
  },
  "preferGlobal": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "start": "node ./build/server.js",
    "dev": "tsx watch src/server.ts",
    "clean": "rimraf build",
    "lint": "echo \"âœ¨ Linting disabled for upstream compatibility - IntelliCommerceâœ¨ Made with ðŸ§¡ in Cape Town ðŸ‡¿ðŸ‡¦\"",
    "lint:fix": "echo \"âœ¨ Linting disabled for upstream compatibility - IntelliCommerceâœ¨ Made with ðŸ§¡ in Cape Town ðŸ‡¿ðŸ‡¦\"",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\" || true",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,md}\" || true",
    "test": "npm run test:unit && npm run test:integration && npm run test:mcp && npm run test:e2e && npm run test:security && npm run test:tools",
    "test:unit": "jest --testPathPattern=tests/server\\.test\\.ts$",
    "test:integration": "jest --testPathPattern=tests/integration\\.test\\.ts$",
    "test:mcp": "jest --testPathPattern=tests/mcp-protocol\\.test\\.ts$",
    "test:e2e": "jest --testPathPattern=tests/e2e\\.test\\.ts$",
    "test:security": "jest --testPathPattern=tests/security\\.test\\.ts$",
    "test:tools": "jest --testPathPattern=tests/tool-descriptions\\.test\\.ts$",
    "test:all": "npm run test",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "npm run test:unit && npm run test:integration && npm run test:mcp && npm run test:e2e && npm run test:security && npm run test:tools",
    "typecheck": "tsc --noEmit",
    "validate": "npm run typecheck && npm run test:all",
    "security:check": "scripts/security-check.sh",
    "structure:check": "scripts/structure-check.sh",
    "deps:check": "scripts/dependencies-update.sh",
    "deps:update:patch": "scripts/dependencies-update.sh patch",
    "deps:update:minor": "scripts/dependencies-update.sh minor",
    "deps:update:major": "scripts/dependencies-update.sh major",
    "deps:update:interactive": "scripts/dependencies-update.sh interactive",
    "deps:audit": "npm audit && npm run security:check",
    "deps:fix": "npm audit fix && npm run deps:audit",
    "deps:outdated": "npm outdated && npx npm-check-updates",
    "deps:fresh": "npm run deps:audit && npm run deps:update:minor && npm run validate",
    "changelog:generate": "scripts/changelog-generator.sh",
    "changelog:deps": "scripts/changelog-generator.sh deps",
    "changelog:dev": "scripts/changelog-generator.sh dev",
    "changelog:update": "scripts/changelog-generator.sh && git add CHANGELOG.md",
    "release:enhanced": "scripts/release-enhanced.sh",
    "release:enhanced:patch": "scripts/release-enhanced.sh patch",
    "release:enhanced:minor": "scripts/release-enhanced.sh minor",
    "release:enhanced:major": "scripts/release-enhanced.sh major",
    "release:check": "scripts/release-enhanced.sh check",
    "pre-publish": "npm run security:check && npm run structure:check && npm run validate",
    "release": "standard-version",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:dry": "standard-version --dry-run",
    "publish:npm": "npm run build && npm publish",
    "publish:npm:dry": "npm run build && npm publish --dry-run",
    "publish:auto": "npm run release && npm run publish:npm && git push --follow-tags origin main",
    "publish:automated": "scripts/publish-automated.sh",
    "publish:automated:dry": "scripts/publish-automated.sh --dry-run",
    "publish:test": "npm run build && npm pack && echo \"Package created successfully for testing\"",
    "husky:install": "husky install",
    "prestart": "npm run build",
    "prepare": "husky",
    "global:install": "npm run build && npm install -g .",
    "global:uninstall": "npm uninstall -g intellicommerce-woo-mcp",
    "global:test": "intellicommerce-woo-mcp --help"
  },
  "keywords": [
    "intellicommerce",
    "woocommerce",
    "mcp",
    "server",
    "claude",
    "ai",
    "copilot",
    "vscode",
    "ecommerce",
    "cape-town",
    "south-africa"
  ],
  "author": {
    "name": "John Guthrie",
    "email": "info@intellicommerce.co.za",
    "url": "https://intellicommerce.co.za"
  },
